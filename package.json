{
  "name": "samp-component-lib",
  "version": "0.0.15",
  "description": "A test ui component library",
  "author": "sam parhizkar",
  "main": "dist/index.js",
  "umd:main": "dist/samp-component-lib.umd.production.min",
  "module": "dist/samp-component-lib.esm",
  "unpkg": "dist/samp-component-lib.umd.production.min",
  "typings": "dist/index.d.ts",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "sideEffects": false,
  "scripts": {
    "prepare": "run-s build",
    "build:rollup": "rollup -c",
    "start": "microbundle-crl watch --no-compress --format modern,cjs,umd",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "lint": "eslint .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:docs": "build-storybook -o docs",
    "clean:docs": "rm -rf docs",
    "clean": "rm -fr dist --jsx",
    "build": "tsdx build --format cjs,esm,umd "
  },
  "dependencies": {
    "lodash-es": "^4.17.15",
    "styled-components": "^5.1.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-a11y": "^6.0.26",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-knobs": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.0.26",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "26.0.14",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "14.11.8",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@types/styled-components": "^5.1.4",
    "@types/tinycolor2": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "axe-core": "4.0.2",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "eslint-tinker": "^0.5.0",
    "husky": "4.3.0",
    "jest": "26.5.3",
    "lint-staged": "10.4.0",
    "microbundle-crl": "^0.13.11",
    "mkdirp": "^1.0.4",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "2.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^17.0.1",
    "react-scripts": "^3.4.4",
    "rimraf": "^3.0.2",
    "tinycolor2": "^1.4.2",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.5",
    "tsdx": "^0.14.1",
    "tslib": "2.0.3",
    "typescript": "4.0.3"
  },
  "repository": {
    "type": "git",
